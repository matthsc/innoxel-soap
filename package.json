{
  "name": "innoxel-soap",
  "version": "0.2.0",
  "description": "innoxel master soap api client",
  "keywords": [
    "innoxel",
    "api",
    "soap"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/innoxel-soap"
  },
  "bugs": "https://github.com/matthsc/innoxel-soap/issues",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha --watch --reporter min",
    "coverage": "nyc mocha",
    "lint": "eslint . --ext .ts",
    "check": "tsc --noEmit && npm run lint && npm test",
    "format": "npm run prettier",
    "prettier": "prettier --config .prettierrc \"{src,test}/**/*.ts\" --write",
    "build": "rimraf dist/* && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2015 --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check",
    "preversion": "npm run lint",
    "release": "dotenv release-it --only-version",
    "version": "npm run prettier && git add -A src"
  },
  "author": "Matthias Schwende (matthsc@gmx.net)",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.0.8",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@types/request": "^2.48.8",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "release-it": "^14.14.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "files": [
    "dist/**/*"
  ],
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "spec": [
      "test/*.spec.ts"
    ],
    "watch-files": [
      "src",
      "test"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "dist/**",
      "src/**/*.spec.ts",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
