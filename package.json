{
  "name": "innoxel-soap",
  "version": "0.1.1",
  "description": "innoxel master soap api client",
  "keywords": [
    "innoxel",
    "api",
    "soap"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/innoxel-soap"
  },
  "bugs": "https://github.com/matthsc/innoxel-soap/issues",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "format": "npm run prettier",
    "prettier": "prettier --config .prettierrc \"{src,test}/**/*.ts\" --write",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:esm": "tsc --module es2015 --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "release": "release-it",
    "version": "npm run prettier && git add -A src"
  },
  "author": "Matthias Schwende (matthsc@gmx.net)",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.0.0-beta.8",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^17.0.2",
    "@types/request": "^2.48.7",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist/**/*"
  ],
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "spec": [
      "test/*.spec.ts"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
