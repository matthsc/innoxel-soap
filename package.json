{
  "name": "innoxel-soap",
  "version": "0.5.0",
  "description": "innoxel master soap api client",
  "keywords": [
    "innoxel",
    "api",
    "soap"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/innoxel-soap"
  },
  "bugs": "https://github.com/matthsc/innoxel-soap/issues",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "check": "run-s check:* lint test",
    "check:tsc": "tsc --noEmit",
    "check:exports": "attw --pack .",
    "format": "npm run prettier",
    "prettier": "prettier --config .prettierrc \"{src,test}/**/*.ts\" --write",
    "build": "tsup",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check",
    "preversion": "npm run check",
    "release": "dotenv release-it --only-version",
    "version": "npm run prettier && git add -A src"
  },
  "author": "Matthias Schwende (matthsc@gmx.net)",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.5.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.6",
    "@types/request": "^2.48.12",
    "@types/request-promise-native": "^1.0.21",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.4",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nock": "^13.5.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist/**/*"
  ],
  "release-it": {
    "github": {
      "release": true
    }
  }
}
